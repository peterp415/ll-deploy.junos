[vsrx]

[vsrx:children]
vsrx-vagrant-left
vsrx-vagrant-right

[vsrx:vars]
# The Vagrant plugin vagrant-junos sets up ssh access as vagrant, so we will use it
# But NETCONF needs the key to be active, so be sure to
#   ssh-add ~/.vagrant.d/insecure_private_key
ansible_ssh_private_key_file=~/.vagrant.d/insecure_private_key
ansible_ssh_user=vagrant

[vsrx-vagrant-left]
# Vagrant srx1-left
srx1-left.vagrant junos_config_hostname=srx1-left.vagrant

[vsrx-vagrant-right]
# Vagrant srx1-right
srx1-right.vagrant junos_config_hostname=srx1-right.vagrant
